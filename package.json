{
  "name": "meta-museum",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "parcel src/index.html && parcel src/select.html --open",
    "clean": "rm -rf dist/*",
    "build:parcel": "parcel build src/index.html src/select.html --no-content-hash --no-minify --no-source-maps --public-url ./",
    "build": "npm run clean && npm run build:parcel"
  },
  "dependencies": {
    "gsap": "^3.6.1",
    "imagesloaded": "^4.1.4",
    "locomotive-scroll": "^4.1.4",
    "parcel": "1.12.3",
    "splitting": "^1.0.6",
    "web3": "^1.7.1"
  },
  "compress": {
    "brotli": {
        "enabled": false
    }
}
}
